{
  "name": "myapptestsentry",
  "version": "1.0.22",
  "private": true,
  "scripts": {
    "postinstall": "patch-package",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "lint-fix": "eslint . --fix",
    "prettier": "prettier ./src/**/* -e ts,tsx --write  ",
    "ios-development": "ENVFILE=.env.development react-native run-ios --scheme MyAppTestSentry[dev] ",
    "ios-staging": "ENVFILE=.env.staging react-native run-ios --scheme MyAppTestSentry[dev] ",
    "ios-uat": "ENVFILE=.env.uat react-native run-ios --scheme MyAppTestSentry[dev] ",
    "ios-production": "ENVFILE=.env.production react-native run-ios --scheme myapptestsentry[production]",
    "android-development": "ENVFILE=.env.development react-native run-android",
    "android-development-windows": "SET ENVFILE = '.env.development' && react-native run-android",
    "android-staging": "ENVFILE=.env.staging react-native run-android",
    "android-uat": "ENVFILE=.env.uat react-native run-android",
    "android-production": "ENVFILE=.env.production react-native run-android",
    "build-local-android-prod": "export ENVFILE=.env.production && cd android && ./gradlew assembleRelease && cd ..",
    "build-local-android-staging": "export ENVFILE=.env.staging && cd android && ./gradlew assembleDebug && cd ..",
    "build-local-android-uat": "export ENVFILE=.env.uat && cd android && ./gradlew assembleDebug && cd ..",
    "build-local-android-development": "export ENVFILE=.env.development && cd android && ./gradlew assembleDebug && cd ..",
    "build-android-local-development": "cd android/ && bundle exec fastlane android local --env development",
    "build-android-local-staging": "cd android/ && bundle exec fastlane android local --env stagin",
    "build-android-local-uat": "cd android/ && bundle exec fastlane android local --env uat",
    "build-android-local-prod": "cd android/ && bundle exec fastlane android local --env production",
    "build-android-development": "cd android/ && bundle exec fastlane android development --env development",
    "build-android-staging": "cd android/ && bundle exec fastlane android staging --env staging",
    "build-android-uat": "cd android/ && bundle exec fastlane android uat --env uat",
    "build-android-production": "cd android/ && bundle exec fastlane android production --env production",
    "build-ios-development": "cd ios/ && bundle exec fastlane ios testflight --env development",
    "build-ios-uat": "cd ios/ && bundle exec fastlane ios testflight --env uat",
    "build-ios-staging": "cd ios/ && bundle exec fastlane ios testflight --env staging",
    "build-ios-production": "cd ios/ && bundle exec fastlane ios testflight --env production",
    "build:iosindex": "react-native bundle --entry-file='index.js' --bundle-output='./ios/main.jsbundle' --dev=false --platform='ios'",
    "build:androidindex": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output ./android/app/src/main/assets/index.android.bundle",
    "reset-cache": "react-native start --reset-cache",
    "clean-gradle": "cd android/ && ./gradlew clean",
    "devtools": "react-devtools",
    "android-avd-list": "emulator -list-avds",
    "test": "jest --verbose",
    "test:update": "jest --verbose --coverage --updateSnapshot",
    "test:watch": "jest --verbose --watch",
    "coverage": "jest --verbose --coverage && open ./coverage/lcov-report/index.html",
    "patch-pods": "./scripts/patch-pods-ios.sh"
  },
